#define LED 16  //piedino GPIO16 ----->>> D0



void setup()
{
  Serial.begin(115200);
  Serial.println();

  pinMode(LED, OUTPUT);
  
  WiFi.begin("GruppoPSC", "protectionUPSC270");

  Serial.print("Connecting");
  while (WiFi.status() != WL_CONNECTED)
  {
    delay(500);
    Serial.print(".");
  }
  Serial.println();

  Serial.print("Connected, IP address: ");
  Serial.println(WiFi.localIP());
  digitalWrite(LED, HIGH);
  
}

